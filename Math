#include <iostream>
#include <cmath>
using namespace std;
int main() {
  int x,y;
  cout << "Give a number. ";
  cin >> x;
  cout << "Give another number. ";
  cin >> y;

  cout << x+y << '\n';
  cout << x-y << '\n';
  cout << x*y << '\n';
  cout << x/y << '\n';
  cout << pow (x,y) << '\n';
  cout << x%y << '\n';
  return 0;
}
